        this.linkedDate = linkedDate;
        this.shownPublicly = shownPublicly;
    }

    public DetailedIntegrationData(final @org.checkerframework.checker.optional.qual.MaybePresent JsonObject json) {
        this(json.get("integration").getAsString(), json.get("identifier").getAsString(), json.get("username").getAsString(), json.get("verified").getAsBoolean(), new Date(json.get("linked_date").getAsLong() * 1000), json.get("show_publicly").getAsBoolean());
    }

    @org.checkerframework.dataflow.qual.Pure
    public final  @org.checkerframework.checker.optional.qual.MaybePresent boolean isVerified() {
        return this.verified;
    }

    @org.checkerframework.dataflow.qual.Pure
    public final @org.checkerframework.checker.optional.qual.MaybePresent Date linkedDate() {
        return this.linkedDate;
    }

    @org.checkerframework.dataflow.qual.Pure
    public final  @org.checkerframework.checker.optional.qual.MaybePresent boolean isShownPublicly() {
        return this.shownPublicly;
    }
}
