package com.namelessmc.java_api.integrations;

import com.google.gson.JsonObject;
import org.checkerframework.checker.nullness.qual.NonNull;
import java.util.Date;

@org.checkerframework.framework.qual.AnnotatedFor("org.checkerframework.checker.optional.OptionalChecker")
public class DetailedIntegrationData extends IntegrationData {

    private final  @org.checkerframework.checker.optional.qual.MaybePresent boolean verified;

    private final @org.checkerframework.checker.optional.qual.MaybePresent Date linkedDate;

    private final  @org.checkerframework.checker.optional.qual.MaybePresent boolean shownPublicly;

    public DetailedIntegrationData(final @org.checkerframework.checker.optional.qual.MaybePresent String integrationType, final @org.checkerframework.checker.optional.qual.MaybePresent String identifier, final @org.checkerframework.checker.optional.qual.MaybePresent String username, final  @org.checkerframework.checker.optional.qual.MaybePresent boolean verified, final @org.checkerframework.checker.optional.qual.MaybePresent Date linkedDate, final  @org.checkerframework.checker.optional.qual.MaybePresent boolean shownPublicly) {
        super(integrationType, identifier, username);
